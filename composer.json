{
    "name": "takeokunn/phpswitch",
    "description": "PHPSwitch API library",
    "license": "MIT",
    "bin": [
        "bin/phpswitch"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "corneltek/pearx": "^1.3.5",
        "corneltek/curlkit": "^1.0.11",
        "ext-simplexml": "*",
        "symfony/yaml": "^5.4",
        "corneltek/cliframework": "3.0.x-dev#01cee2d491085cfae63c677611b5e4604624771e"
    },
    "require-dev": {
        "php-vcr/php-vcr": "^1.5.4",
        "phpunit/phpunit": "^7.5 || ^8.5"
    },
    "autoload": {
        "psr-4": {
            "PhpSwitch\\": "src/PhpSwitch/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSwitch\\Tests\\": "tests/PhpSwitch/"
        }
    }
}
